{
  "name": "@llm-dev-ops/connector-hub-cli",
  "version": "0.1.0",
  "description": "Command-line interface for LLM Connector Hub",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "llm-hub": "./bin/llm-hub.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/globalbusinessadvisors/llm-connector-hub",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "tsc -b && chmod +x bin/llm-hub.js",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "cli",
    "command-line",
    "openai",
    "anthropic",
    "google-ai"
  ],
  "license": "MIT OR Apache-2.0",
  "dependencies": {
    "@llm-dev-ops/connector-hub-core": "^0.1.0",
    "@llm-dev-ops/connector-hub": "^0.1.0",
    "@llm-dev-ops/connector-hub-middleware": "^0.1.0",
    "@llm-dev-ops/connector-hub-providers": "^0.1.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^8.2.10",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}
